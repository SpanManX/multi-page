{
  "name": "multi-page",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:All": "vue-cli-service build --page1 --page2",
    "build:page1": "vue-cli-service build --page1",
    "build:page2": "vue-cli-service build --page2",
    "serve:All": "vue-cli-service serve --page1 --page2",
    "serve:page1": "vue-cli-service serve --page1",
    "serve:page2": "vue-cli-service serve --page2"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "echarts": "^5.1.2",
    "element-ui": "^2.14.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.9",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
